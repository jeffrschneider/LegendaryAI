<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<servlet>
		<servlet-name>native</servlet-name>
		<servlet-class>ai.legendary.services.PageServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>scs</servlet-name>
		<servlet-class>ai.legendary.services.SentenceCorrectionService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>scs</servlet-name>
		<url-pattern>/WordTupleConnection/wordCorrect/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>native</servlet-name>
		<url-pattern>/WordTupleConnection/*</url-pattern>
	</servlet-mapping>
</web-app>